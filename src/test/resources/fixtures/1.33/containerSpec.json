{
  "Image": "nginx:alpine",
  "Mounts": [
    {
      "ReadOnly": true,
      "Source": "web-data",
      "Target": "/usr/share/nginx/html",
      "Type": "volume",
      "VolumeOptions": {
        "DriverConfig": { },
        "Labels": {
          "com.example.something": "something-value"
        }
      }
    }
  ],
  "Hosts": [
    "10.10.10.10 host1",
    "ABCD:EF01:2345:6789:ABCD:EF01:2345:6789 host2"
  ],
  "User": "33",
  "DNSConfig": {
    "Nameservers": [
      "8.8.8.8"
    ],
    "Search": [
      "example.org"
    ],
    "Options": [
      "timeout:3"
    ]
  },
  "Secrets": [
    {
      "File": {
        "Name": "www.example.org.key",
        "UID": "33",
        "GID": "33",
        "Mode": 384
      },
      "SecretID": "fpjqlhnwb19zds35k8wn80lq9",
      "SecretName": "example_org_domain_key"
    }
  ]
}
